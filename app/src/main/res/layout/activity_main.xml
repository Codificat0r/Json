<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.example.xmlacdat.MainActivity"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <Button
        android:id="@+id/btn_Primitiva"
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:onClick="onClick_lanzarExs"
        android:text="Primitiva"/>

    <Button
        android:id="@+id/btn_Contactos"
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:onClick="onClick_lanzarExs"
        android:text="Contactos"/>

    <Button
        android:id="@+id/btn_ContactosGson"
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:onClick="onClick_lanzarExs"
        android:text="Contactos Gson"/>

    <Button
        android:id="@+id/btn_CreacionJSON"
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:onClick="onClick_lanzarExs"
        android:text="Creacion JSON"/>

    <Button
        android:id="@+id/btn_Repositorios"
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:onClick="onClick_lanzarExs"
        android:text="Repositorios"/>
</LinearLayout>
